<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="tk.Toplevel" id="mainwindow">
    <property name="background">#400040</property>
    <property name="container">false</property>
    <property name="resizable">both</property>
    <property name="takefocus">false</property>
    <property name="title" translatable="yes">Hello World App</property>
    <child>
      <object class="ttk.Frame" id="mainframe">
        <property name="relief">raised</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="tk.Frame" id="setupFrame">
            <property name="background">#ffffff</property>
            <property name="borderwidth">5</property>
            <property name="height">0</property>
            <property name="highlightcolor">#ffffff</property>
            <property name="width">0</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="padx">30</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
            <child>
              <object class="pygubu.builder.widgets.calendarframe" id="calendarframe2">
                <property name="firstweekday">6</property>
                <property name="month">1</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Frame" id="frame1">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">2</property>
                </layout>
                <child>
                  <object class="ttk.Label" id="label3">
                    <property name="cursor">arrow</property>
                    <property name="takefocus">false</property>
                    <property name="text" translatable="yes">Time 24h: </property>
                    <layout manager="grid">
                      <property name="column">0</property>
                      <property name="propagate">True</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Spinbox" id="Time">
                    <property name="cursor">arrow</property>
                    <property name="font">TkDefaultFont</property>
                    <property name="increment">1</property>
                    <property name="takefocus">true</property>
                    <property name="text" translatable="yes">Time Picker</property>
                    <layout manager="grid">
                      <property name="column">1</property>
                      <property name="propagate">True</property>
                      <property name="row">0</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="ttk.Button" id="submitButton">
                <property name="style">Toolbutton</property>
                <property name="text" translatable="yes">Submit</property>
                <bind sequence="&lt;submitButton&gt;" handler="buttonClick" add="True" />
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="pady">30</property>
                  <property name="propagate">True</property>
                  <property name="row">3</property>
                  <property name="sticky">s</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="postFrame">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="padx">10</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
            <child>
              <object class="ttk.Label" id="postTitle">
                <property name="text" translatable="yes">Post Title</property>
                <layout manager="pack">
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Entry" id="titleEntry">
                <property name="exportselection">true</property>
                <property name="font">TkDefaultFont</property>
                <property name="justify">left</property>
                <property name="state">normal</property>
                <layout manager="pack">
                  <property name="expand">false</property>
                  <property name="fill">x</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="descriptionLabel">
                <property name="text" translatable="yes">Description</property>
                <layout manager="pack">
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Text" id="descriptionText">
                <property name="font">{@MingLiU_HKSCS-ExtB} 12 {}</property>
                <property name="height">10</property>
                <property name="relief">raised</property>
                <property name="setgrid">false</property>
                <property name="takefocus">false</property>
                <property name="width">50</property>
                <layout manager="pack">
                  <property name="ipadx">0</property>
                  <property name="ipady">0</property>
                  <property name="padx">0</property>
                  <property name="pady">0</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Frame" id="frame2">
                <property name="height">200</property>
                <property name="width">200</property>
                <layout manager="pack">
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
                <child>
                  <object class="tk.Canvas" id="uploadCanvas">
                    <layout manager="pack">
                      <property name="padx">10</property>
                      <property name="pady">10</property>
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
                <child>
                  <object class="ttk.Button" id="uploadButton">
                    <property name="text" translatable="yes">Upload Photo</property>
                    <layout manager="pack">
                      <property name="propagate">True</property>
                      <property name="side">top</property>
                    </layout>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="viewFrame">
            <property name="height">200</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">3</property>
              <property name="padx">30</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
            </layout>
            <child>
              <object class="pygubu.builder.widgets.editabletreeview" id="eventsList">
                <property name="height">25</property>
                <property name="selectmode">extended</property>
                <property name="show">tree</property>
                <layout manager="pack">
                  <property name="fill">y</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="logoutButton">
            <property name="text" translatable="yes">Logout</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
